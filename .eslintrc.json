{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "comma-dangle": "off",
    "max-len": ["error", {
      "code": 140
    }],
    "object-curly-newline": "off",
    "quotes": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
